{
  "manifest_version": 3,
  "name": "Obsidian Simple Read Later",
  "description": "Save URLs to your Obsidian vault to read them later with a click of a button!",
  "version": "1.0.0",
  "action": {
    "default_title": "Save URL to Obsidian"
  },
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["http://127.0.0.1:27123/vault/*"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "128": "icon.png"
  }
}
